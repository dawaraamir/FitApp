{
  "name": "fitness-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "backend:install": "node tools/install-backend-deps.js",
    "backend": "uvicorn backend.main:app --host 0.0.0.0 --port 8080 --reload",
    "mobile:sync": "NODE_OPTIONS=--openssl-legacy-provider npm run build && npx cap sync",
    "mobile:open:ios": "npm run mobile:sync && npx cap open ios",
    "mobile:open:android": "npm run mobile:sync && npx cap open android"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/fire": "^6.1.5",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^12.1.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@capacitor/core": "^5.8.0",
    "angular-google-charts": "^2.2.2",
    "firebase": "^7.0 || ^8.0",
    "rxjs": "~6.6.0",
    "sortable-tables": "^1.1.2",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1300",
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@capacitor/android": "^5.8.0",
    "@capacitor/cli": "^5.8.0",
    "@capacitor/ios": "^5.8.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "firebase-tools": "^8.0.0 || ^9.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.7.0",
    "jsonc-parser": "^3.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "open": "^7.0.3",
    "typescript": "~4.2.3"
  }
}
